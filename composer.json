{
    "name": "proklung/framework-tools-bundle",
    "description": "Misc tools for custom Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/framework-tools-bundle/issues",
        "source": "https://github.com/ProklUng/framework-tools-bundle"
    },
    "autoload": {
        "psr-4": {
            "Prokl\\FrameworkExtensionBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=7.3",
        "psr/container": "^1.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0",
        "symfony/config": "^4.0 || ^5.0",
        "symfony/console": "^4.0 || ^5.0",
        "symfony/process": "^4.0 || ^5.0",
        "symfony/filesystem": "^4.0 || ^5.0",
        "symfony/lock": "^4.0 || ^5.0",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "doctrine/common": "^3.1",
        "denismitr/translit": "~3"
    },
    "require-dev": {
        "proklung/phpunit-testing-tools": "^1.3.0"
    }
}
